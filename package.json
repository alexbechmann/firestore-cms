{
  "name": "ssr",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "cli-consumer"
  ],
  "scripts": {
    "start:dev": "cd dev && npm start",
    "start:cli": "cd cli-consumer && npm start",
    "start": "npm run start:cli",
    "test": "ts-mocha --paths -p ./__tests__/tsconfig.json __tests__/**/*.test*.ts",
    "t": "tsc -p __tests__ && ",
    "build": "npm-run-all build:js build:types:core build:types:dashboard build:types:server",
    "test:watch": "yarn run test --watch --watch-extensions ts",
    "build:js": "webpack --config webpack.config.packages.js",
    "build:types:core": "tsc -p ./packages/core --emitDeclarationOnly",
    "build:types:dashboard": "tsc -p ./packages/dashboard --emitDeclarationOnly",
    "build:types:server": "tsc -p ./packages/server --emitDeclarationOnly"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/webpack-env": "^1.13.6",
    "chai": "4.1.2",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^5.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "^1.15.2",
    "prettier-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.2",
    "ts-mocha": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.4.5",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
