{
  "name": "ssr",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "workspaces": [
    "consumer",
    "packages/*"
  ],
  "scripts": {
    "start": "npm-run-all -p consumer:start core:start dashboard:start server:start",
    "postinstall": "npm-run-all core:build dashboard:build server:build consumer:build",
    "consumer:build": "cd ./consumer && yarn run build",
    "consumer:start": "cd ./consumer && yarn run start",
    "core:start": "cd ./packages/core && yarn run start",
    "core:build": "cd ./packages/core && yarn run build",
    "dashboard:start": "cd ./packages/dashboard && yarn run start",
    "dashboard:build": "cd ./packages/dashboard && yarn run build",
    "server:start": "cd ./packages/server && yarn run start",
    "server:build": "cd ./packages/server && yarn run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}
